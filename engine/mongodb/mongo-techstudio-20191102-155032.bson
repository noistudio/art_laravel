  "_id": ObjectId("589c894cb83a886ce6190aaa"),
  "user": {
    "uid": "100001897470090",
    "photo": "http://graph.facebook.com/100001897470090/picture?type=square",
    "identity": "https://www.facebook.com/app_scoped_user_id/100001897470090/",
    "network": "facebook",
    "profile": "https://www.facebook.com/app_scoped_user_id/100001897470090/",
    "first_name": "Kate",
