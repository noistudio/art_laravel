  "_id": ObjectId("58a29304b83a8877d31a9296"),
  "title": "Про старовойтова",
  "image": "/files/tmpfiles/fc87f7633e5c81cffa2113df5f12be19.jpg",
  "date": ISODate("2017-02-13T21:00:00.0Z"),
